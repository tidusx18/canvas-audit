<div class="entry">
  <div class="body">
    <h1>{{data.title}}</h1>
    <hr>
    {{#each data}}
    <div class="module">
      <h2 class="module-title">{{name}}</h2>
      <div class="module-items">
      {{#each moduleItems}}
      {{#if published}}
      {{#if html_url}}<a target="_blank" href={{html_url}}>{{/if}}
      <h3 class="module-item-title published">Published | {{type}} : {{title}}</h3>
      {{#if html_url}}</a>{{/if}}
      {{/if}}
      {{#unless published}}
      {{#if html_url}}<a target="_blank" href={{html_url}}>{{/if}}
      <h3 class="module-item-title unpublished">Unpublished | {{type}} : {{title}}</h3>
      {{#if html_url}}</a>{{/if}}
      {{/unless}}
      {{#with auditResults}}
        <div class="audit-results">
        {{#if videos}}<p class="notice">Review Video for Captions</p>{{/if}}
        {{#tableWarning tables}}{{/tableWarning}}
        {{#textWarnings text}}{{/textWarnings}}
        {{#if links}}
        <h4>LINKS:</h4>
        {{#each links}}
        <ul>
          <li><span class="property-name">Href:</span> <a target="_blank" href={{href}}>{{href}}</a></li>
          <li><span class="property-name">Anchor Text:</span> {{anchorText}}</li>
          {{#hasTitle this}}{{/hasTitle}}
          {{#isContextual contextualAnchorText}}{{/isContextual}}
          {{#bbRef bbReference}}{{/bbRef}}
          {{#pearsonRef pearsonReference}}{{/pearsonRef}}
          {{#isReachable isReachable}}{{/isReachable}}
          {{#unless @last}}<hr>{{/unless}}
        </ul>
        {{/each}}
        {{/if}}
        {{#if images}}
        <h4>IMAGES:</h4>
        {{#each images}}
        <ul>
          {{#hasAlt alt}}{{/hasAlt}}
          <li><span class="property-name">Src:</span> {{src}}</li>
        </ul>
        {{#unless @last}}<hr>{{/unless}}
        {{/each}}
        {{/if}}
        </div>
      {{/with}}
      {{/each}}
      </div>
    </div>
    {{/each}}
</div>